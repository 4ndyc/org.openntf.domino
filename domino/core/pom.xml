<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>org.openntf.domino</groupId>
		<artifactId>domino</artifactId>
		<version>latest</version>
	</parent>
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
	</properties>
	<artifactId>core</artifactId>
	<name>OpenNTF Domino API</name>
	<description>The drop-in replacement for the IBM-supported lotus.domino API</description>
	<build>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<organization>
		<name>OpenNTF</name>
		<url>http://www.openntf.org</url>
	</organization>
	<dependencies>
		<dependency>
			<groupId>org.openntf.domino</groupId>
			<artifactId>formula</artifactId>
			<version>latest</version>
		</dependency>
		<dependency>
			<groupId>org.openntf.domino</groupId>
			<artifactId>asm</artifactId>
			<version>latest</version>
		</dependency>
		<dependency>
			<groupId>org.openntf.domino</groupId>
			<artifactId>jsr305</artifactId>
			<version>latest</version>
		</dependency>
		<dependency>
			<groupId>org.openntf.domino</groupId>
			<artifactId>javassist</artifactId>
			<version>latest</version>
		</dependency>
		<dependency>
			<groupId>org.openntf.domino</groupId>
			<artifactId>guava</artifactId>
			<version>latest</version>
		</dependency>
		<dependency>
			<groupId>org.openntf.domino</groupId>
			<artifactId>javolution</artifactId>
			<version>latest</version>
		</dependency>
		<dependency>
			<groupId>org.openntf.domino</groupId>
			<artifactId>rxjava</artifactId>
			<version>latest</version>
		</dependency>
		<dependency>
			<groupId>org.openntf.domino</groupId>
			<artifactId>tinkerpop</artifactId>
			<version>latest</version>
		</dependency>
		<dependency>
			<groupId>com.ibm.sbt</groupId>
			<artifactId>com.ibm.commons</artifactId>
			<version>9.0.0</version>
		</dependency>
		<dependency>
			<groupId>ibm.domino</groupId>
			<artifactId>notes</artifactId>
			<version>9.0.1</version>
		</dependency>
		<dependency>
			<groupId>ibm.domino</groupId>
			<artifactId>jvmRuntime</artifactId>
			<version>9.0.1</version>
		</dependency>
	</dependencies>
</project>