							<target>
								<property name="dlURL" value="${domino.buildmanagement.url}" />
								<echo message="Downloading 'IBM Domino Update Site for Build Management' from ${dlURL}" />
								-------------------------------------------------------------------
								<echo message="" />
								<echo message="    #============================================================#" />
								<echo message="    # Note that this submission is distributed under a           #" />
								<echo message="    # proprietary license and is not open source.                #" />
								<echo message="    # It has been posted to the OpenNTF, pursuant to the OpenNTF #" />
								<echo message="    # IP Policy, to assist OpenNTF users develop software.       #" />
								<echo message="    #============================================================#" />
								<echo message="" />
								<mkdir dir="download" />
								<!-- See details: http://openntf.org/main.nsf/project.xsp?r=project/IBM%20Domino%20Update%20Site%20for%20Build%20Management 
									Original DL-URL (Version is from 2014/18/11, MD5: 8baf3908437dffc5661112148ff96cfc) 
									 
									RPr: OpenNTF is sometimes so slow - so I (temporary!?) mirrored the file 
									here: https://www.dropbox.com/s/aqy3kua8lzin6iz/DominoBuildManagementUpdateSite.zip?dl=1 
									File is put in "download" folder, so that it is not deleted on "mvn clean" -->
								<get
									src="${dlURL}"
									dest="download/DominoBuildManagementUpdateSite.zip"
									skipexisting="true" verbose="true" />

								<echo message="MD5 check..." />
								<property name="md5Expected" value="8baf3908437dffc5661112148ff96cfc" />
								<checksum file="download/DominoBuildManagementUpdateSite.zip"
									property="md5Zip" />
								<echo message="MD5 OK... converting to m2 repository" />
								<fail
									message="The MD5 of the ZIP-file does not match.${line.separator}got:      ${md5Zip}${line.separator}expected: ${md5Expected}${line.separator}To download again, you have to delete 'download/DominoBuildManagementUpdateSite.zip'">
									<condition>
										<not>
											<equals arg1="${md5Zip}" arg2="${md5Expected}" />
										</not>
									</condition>
								</fail>

								<!-- Unzip nightmare -->
								<unzip src="download/DominoBuildManagementUpdateSite.zip"
									dest="target/" />
								<unzip src="target/UpdateSite.zip" dest="target/UpdateSite" />
								<unzip dest="target/innerJars">
									<fileset dir="target/UpdateSite/plugins/">
										<include name="*.jar" />
									</fileset>
									<patternset>
										<include name="*.jar" />
										<include name="lib/*.jar" />
									</patternset>
								</unzip>
							</target>
						</configuration>
						<goals>
							<goal>run</goal>
						</goals>
					</execution>
				</executions>
			</plugin>


